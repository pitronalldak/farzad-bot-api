{
  "name": "bot-back",
  "version": "0.1.0",
  "description": "bot-back",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0 --plugins transform-decorators-legacy\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0 --plugins transform-decorators-legacy",
    "start": "node dist",
    "postinstall": "npm run build",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "bot-back",
  "author": "Petr Ryabinin",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.13.3",
    "co": "^4.6.0",
    "compression": "^1.5.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "crypto-js": "^3.1.9-1",
    "eslint": "^3.1.1",
    "express": "^4.13.3",
    "express-session": "^1.14.2",
    "express-validator": "^3.1.2",
    "fs": "0.0.1-security",
    "google-auth-library": "^0.10.0",
    "googleapis": "^19.0.0",
    "moment": "^2.18.1",
    "mongoose": "^4.9.8",
    "node-telegram-bot-api": "^0.27.1",
    "nodemon": "^1.9.2",
    "path": "^0.12.7",
    "pg": "^6.1.2",
    "pg-promise": "^5.5.2",
    "readline": "^1.3.0",
    "reflect-metadata": "^0.1.9",
    "resource-router-middleware": "^0.5.1",
    "typeorm": "0.0.6",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
